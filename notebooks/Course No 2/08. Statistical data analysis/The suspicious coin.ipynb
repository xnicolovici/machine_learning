   "source": [
    "from scipy.special import binom\n",
    "import numpy as np\n",
    "\n",
    "def p_y_le(k):\n",
    "    return sum([binom(100, i)*np.power(0.5, 100) for i in range(1, k+1)])\n",
    "\n",
    "p_y_le(27) + (1 - p_y_le(72))"
   ]
